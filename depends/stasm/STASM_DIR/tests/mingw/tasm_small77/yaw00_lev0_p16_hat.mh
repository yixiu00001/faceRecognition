static double linmod(
    const double* const x,
    const double        intercept,
    const double* const coef,
    const int           n)
{
    double yhat = intercept;
#if 0                               // unoptimized
    for (int i = 0; i < n; i++)
        yhat += coef[i] * x[i];
#else                               // optimized
    const int n4 = n - 4;
    int i = 0;
    if (n4 > 0)
    {
        for (; i < n4; i += 4)
            yhat += coef[i]   * x[i]   +
                    coef[i+1] * x[i+1] +
                    coef[i+2] * x[i+2] +
                    coef[i+3] * x[i+3];
    }
    for (; i < n; i++)
        yhat += coef[i] * x[i];
#endif
    return yhat;
}

